{
  "name": "myappjs",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run database-init && ts-node ./src",
    "dev": "nodemon",
    "lint": "tslint --project tsconfig.json -c tslint.json 'src/**/*.ts'",
    "test": "npm run lint && npm run unit-test && npm run integration-test",
    "unit-test": "mocha --require \"ts-node/register\" \"src/**/*.test.ts\"",
    "build-integration-test": "mocha --require \"ts-node/register\" \"test/**.test.ts\" --exit",
    "integration-test": "npm run database-test && npm run build-integration-test",
    "database-init": "npm run database-schema && npm run database-seeds",
    "database-schema": "ts-node src/server/db/schema/index.ts",
    "database-seeds": "ts-node src/server/db/loadSeeds.ts",
    "database-test": "cross-env PROFILE=test npm run database-schema && npm run database-seeds-test",
    "database-seeds-test": " cross-env PROFILE=test ts-node src/server/db/loadSeedsTest.ts"
  },
