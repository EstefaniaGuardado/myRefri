{
  "name": "myappjs",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run postinstall && npm run database-connection && node index.js",
    "test": "npm run tslint && npm run unit-test && npm run integration-test",
    "unit-test": "mocha --require \"ts-node/register\" \"src/**/*.test.ts\"",
    "postinstall": "npm run build-ts",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "dev": "set PORT=8000 && nodemon index.js",
    "integration-test": "set PROFILE=test&& npm run database-dev && npm run database-seeds-test && npm run build-integration-test",
    "build-integration-test": "mocha --require \"ts-node/register\" \"test/**.test.ts\" --exit",
    "build-ts": "tsc",
    "database-connection": "node src/server/db/connector.js",
    "database-dev": "node src/server/db/schema/index.js",
    "database-seeds": "node src/server/db/loadSeeds.js",
    "database-seeds-test": "node src/server/db/loadSeedsTest.js"
  },
